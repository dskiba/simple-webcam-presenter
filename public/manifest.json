{
  "manifest_version": 3,
  "name": "Simple Webcam Presenter",
  "description": "A easy way to show your webcam in floating window",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "camera3.png",
    "48": "camera3.png",
    "128": "camera3.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Shift+W"
      }
    }
  }
}
